<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DarkThemeSwitch" parent="Widget.Material3.CompoundButton.Switch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/settings_line_image_margin_start</item>
        <item name="android:layout_marginEnd">@dimen/common_indent_16</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:trackTint">@color/yp_blue</item>
        <item name="android:thumbTint">@color/yp_blue</item>
